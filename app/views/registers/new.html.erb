<%= simple_form_for @register, url: job_registers_path(@job) do |f| %>
  <%= f.input :full_name %>
  <%= f.input :phone_number %>
  <%= f.input :email %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :zip %>
  <%= f.input :month %>
  <%= f.input :highschool %>
  <%= f.input :college %>
  <%= f.input :location %>
  <%= f.input :years %>
  <%= f.input :major %>
  <%= f.input :company_name %>
  <%= f.input :job_title %>
  <%= f.input :description %>
  <%= f.input :goals %>
  <%= f.submit "APPLY", class: 'btn btn-primary' %>
<% end %>
